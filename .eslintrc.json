{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "plugin:tailwindcss/recommended",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "plugins": [
    "import",
    "unused-imports"
  ],
  "rules": {
    "unused-imports/no-unused-imports": "error",
    "import/no-duplicates": "error"
  }
}
